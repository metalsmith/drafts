{
  "name": "@metalsmith/drafts",
  "version": "1.1.1",
  "description": "A metalsmith plugin to hide drafts.",
  "keywords": [
    "drafts",
    "metalsmith",
    "metalsmith-plugin",
    "static-site"
  ],
  "homepage": "https://github.com/metalsmith/drafts#readme",
  "bugs": {
    "url": "https://github.com/metalsmith/drafts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metalsmith/drafts.git"
  },
  "license": "MIT",
  "author": "Ian Storm Taylor (https://ianstormtaylor.com)",
  "contributors": [
    "Andrew Goodricke",
    "Kevin Van Lierde (https://webketje.com)"
  ],
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib",
    "CHANGELOG.md"
  ],
  "scripts": {
    "changelog": "auto-changelog -u --starting-version v1.1.1 --commit-limit false --ignore-commit-pattern '(dev|ci|chore|Release|Merge)'",
    "coverage": "nyc report --reporter=text-lcov > ./coverage.info",
    "coveralls": "npm run coverage && cat ./coverage.info | coveralls",
    "format": "prettier --write \"**/*.{yml,md,js,json}\"",
    "lint": "eslint --cache --fix-dry-run .",
    "release": "release-it .",
    "test": "nyc mocha"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.1.0",
    "auto-changelog": "^2.4.0",
    "coveralls": "^3.1.1",
    "del": "^5.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "metalsmith": "^2.3.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "release-it": "^15.2.0"
  },
  "peerDependencies": {
    "metalsmith": "^2.3.0"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}
